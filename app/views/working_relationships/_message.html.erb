<div data-working-relationship-subscription-target="message" data-sender-id="<%= message.user.id %>" class="message-cnt">
  <div id="message-<%= message.id %>">
    <small>
      <strong><%= message.user.first_name %></strong>
      <i><%= message.created_at.strftime("%d/%m/%y at %l:%M %p") %></i>
    </small>
    <p><%= message.content %></p>
  </div>
</div>
